"$ export ROS_DOMAIN_ID=42\n"
"$ ros2 run turtlesim turtlesim_node --ros-args --remap \"__node:"
"=my_turtle\"\n"
"[INFO] [1742150439.022947971] [my_turtle]: Starting turtlesim with node "
"name /my_turtle\n"
"[INFO] [1742150439.026043867] [my_turtle]: Spawning turtle [turtle1] at "
"x=[5.544445], y=[5.544445], theta=[0.000000]"
msgstr ""

