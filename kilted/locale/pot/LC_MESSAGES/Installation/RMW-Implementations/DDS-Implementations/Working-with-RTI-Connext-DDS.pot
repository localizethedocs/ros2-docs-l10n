msgid ""
"ROS 2 users can install a non-commercial-use version of the RTI Connext DDS "
"libraries from the ROS apt repository using the following command:"
msgstr ""

#: ../../../source/Installation/RMW-Implementations/DDS-Implementations/Working-with-RTI-Connext-DDS.rst:35
msgid "v7.3.0"
msgstr ""

#: ../../../source/Installation/RMW-Implementations/DDS-Implementations/Working-with-RTI-Connext-DDS.rst:37
