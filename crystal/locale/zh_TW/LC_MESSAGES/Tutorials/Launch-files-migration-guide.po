#: ../../../source/Tutorials/Launch-files-migration-guide.rst:22
msgid "launch"
msgstr ""

#: ../../../source/Tutorials/Launch-files-migration-guide.rst:24
msgid "`Available in ROS 1 <https://wiki.ros.org/roslaunch/XML/launch>`__."
msgstr ""

#: ../../../source/Tutorials/Launch-files-migration-guide.rst:25
msgid "``launch`` is the root element of any ROS 2 launch XML file."
msgstr ""

#: ../../../source/Tutorials/Launch-files-migration-guide.rst:28
msgid "node"
msgstr ""

#: ../../../source/Tutorials/Launch-files-migration-guide.rst:30
msgid "`Available in ROS 1 <https://wiki.ros.org/roslaunch/XML/node>`__."
msgstr ""

#: ../../../source/Tutorials/Launch-files-migration-guide.rst:31
msgid "Launches a new node."
msgstr ""

#: ../../../source/Tutorials/Launch-files-migration-guide.rst:32
#: ../../../source/Tutorials/Launch-files-migration-guide.rst:169
#: ../../../source/Tutorials/Launch-files-migration-guide.rst:190
#: ../../../source/Tutorials/Launch-files-migration-guide.rst:255
msgid "Differences from ROS 1:"
msgstr ""

#: ../../../source/Tutorials/Launch-files-migration-guide.rst:33
msgid "``type`` attribute is now ``exec``."
msgstr ""

#: ../../../source/Tutorials/Launch-files-migration-guide.rst:34
