#: ../../../source/Tutorials/Intermediate/Launch/Using-Substitutions.rst:208
msgid ""
"<?xml version=\"1.0\" encoding=\"UTF-8\"?>\n"
"<launch>\n"
"  <arg name=\"turtlesim_ns\" default=\"turtlesim1\" />\n"
"  <arg name=\"use_provided_red\" default=\"False\" />\n"
"  <arg name=\"new_background_r\" default=\"200\" />\n"
"\n"
"  <node pkg=\"turtlesim\" namespace=\"$(var turtlesim_ns)\" "
"exec=\"turtlesim_node\" name=\"sim\" />\n"
"  <executable cmd=\"ros2 service call $(var turtlesim_ns)/spawn turtlesim/"
"srv/Spawn '{x: 5, y: 2, theta: 0.2}'\" />\n"
"  <executable cmd=\"ros2 param set $(var turtlesim_ns)/sim background_r "
"120\" />\n"
"  <timer period=\"2.0\">\n"
"    <executable cmd=\"ros2 param set $(var turtlesim_ns)/sim background_r "
"$(var new_background_r)\"\n"
"      if=\"$(eval '$(var new_background_r) == 200 and $(var "
"use_provided_red)')\" />\n"
"  </timer>\n"
"</launch>\n"
msgstr ""

