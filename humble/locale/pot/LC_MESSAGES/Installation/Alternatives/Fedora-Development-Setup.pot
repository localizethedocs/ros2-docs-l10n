#: ../../../source/Installation/Alternatives/Fedora-Development-Setup.rst:11
msgid ""
"The following system dependencies are required to build ROS 2 on Fedora. "
"They can be installed with ``dnf`` as follows:"
msgstr ""

#: ../../../source/Installation/Alternatives/Fedora-Development-Setup.rst:14
msgid ""
"sudo dnf install \\\n"
"  cmake \\\n"
"  cppcheck \\\n"
"  eigen3-devel \\\n"
"  gcc-c++ \\\n"
"  liblsan \\\n"
"  libXaw-devel \\\n"
"  libyaml-devel \\\n"
"  make \\\n"
"  opencv-devel \\\n"
"  patch \\\n"
"  python3-colcon-common-extensions \\\n"
"  python3-coverage \\\n"
"  python3-devel \\\n"
"  python3-empy \\\n"
"  python3-nose \\\n"
"  python3-pip \\\n"
"  python3-pydocstyle \\\n"
"  python3-pyparsing \\\n"
"  python3-pytest \\\n"
"  python3-pytest-cov \\\n"
"  python3-pytest-mock \\\n"
"  python3-pytest-runner \\\n"
"  python3-rosdep \\\n"
"  python3-setuptools \\\n"
"  python3-vcstool \\\n"
"  poco-devel \\\n"
"  poco-foundation \\\n"
"  python3-flake8 \\\n"
"  python3-flake8-import-order \\\n"
"  redhat-rpm-config \\\n"
"  uncrustify \\\n"
"  wget"
msgstr ""

