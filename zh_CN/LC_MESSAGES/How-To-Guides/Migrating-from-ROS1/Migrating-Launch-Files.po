"<launch>\n"
"   <arg name=\"use_time_prefix_in_talker\" default=\"0\"/>\n"
"   <group>\n"
"      <let name=\"launch-prefix\" value=\"time\" if=\"$(var "
"use_time_prefix_in_talker)\"/>\n"
"      <node pkg=\"demo_nodes_cpp\" exec=\"talker\"/>\n"
"   </group>\n"
