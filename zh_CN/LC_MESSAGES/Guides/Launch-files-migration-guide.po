#: ../../../source/Guides/Launch-files-migration-guide.rst:23
msgid "Migrating tags from ROS 1 to ROS 2"
msgstr ""

#: ../../../source/Guides/Launch-files-migration-guide.rst:26
msgid "launch"
msgstr ""

#: ../../../source/Guides/Launch-files-migration-guide.rst:28
msgid "`Available in ROS 1 <https://wiki.ros.org/roslaunch/XML/launch>`__."
msgstr ""

#: ../../../source/Guides/Launch-files-migration-guide.rst:29
msgid "``launch`` is the root element of any ROS 2 launch XML file."
msgstr ""

#: ../../../source/Guides/Launch-files-migration-guide.rst:32
msgid "node"
msgstr ""

#: ../../../source/Guides/Launch-files-migration-guide.rst:34
msgid "`Available in ROS 1 <https://wiki.ros.org/roslaunch/XML/node>`__."
msgstr ""

#: ../../../source/Guides/Launch-files-migration-guide.rst:35
msgid "Launches a new node."
msgstr ""

#: ../../../source/Guides/Launch-files-migration-guide.rst:36
#: ../../../source/Guides/Launch-files-migration-guide.rst:173
#: ../../../source/Guides/Launch-files-migration-guide.rst:194
#: ../../../source/Guides/Launch-files-migration-guide.rst:259
msgid "Differences from ROS 1:"
msgstr ""

#: ../../../source/Guides/Launch-files-migration-guide.rst:37
msgid "``type`` attribute is now ``exec``."
msgstr ""

#: ../../../source/Guides/Launch-files-migration-guide.rst:38
