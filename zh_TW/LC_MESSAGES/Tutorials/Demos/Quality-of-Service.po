msgstr ""

#: ../../../source/Tutorials/Demos/Quality-of-Service.rst:59
msgid "macOS"
msgstr "macOS"

#: ../../../source/Tutorials/Demos/Quality-of-Service.rst:65
msgid "Windows"
msgstr "Windows"

#: ../../../source/Tutorials/Demos/Quality-of-Service.rst:67
