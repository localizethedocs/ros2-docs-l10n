msgid "Linux"
msgstr "Linux"

#: ../../../source/Installation/Maintaining-a-Source-Checkout.rst:27
#: ../../../source/Installation/Maintaining-a-Source-Checkout.rst:35
msgid ""
"cd ~/ros2_ws\n"
"mv -i ros2.repos ros2.repos.old\n"
"wget https://raw.githubusercontent.com/ros2/ros2/eloquent-release/ros2.repos"
msgstr ""

#: ../../../source/Installation/Maintaining-a-Source-Checkout.rst:33
#: ../../../source/Installation/Maintaining-a-Source-Checkout.rst:70
#: ../../../source/Installation/Maintaining-a-Source-Checkout.rst:105
#: ../../../source/Installation/Maintaining-a-Source-Checkout.rst:151
#: ../../../source/Installation/Maintaining-a-Source-Checkout.rst:194
msgid "macOS"
msgstr "macOS"

#: ../../../source/Installation/Maintaining-a-Source-Checkout.rst:41
#: ../../../source/Installation/Maintaining-a-Source-Checkout.rst:78
#: ../../../source/Installation/Maintaining-a-Source-Checkout.rst:113
#: ../../../source/Installation/Maintaining-a-Source-Checkout.rst:158
#: ../../../source/Installation/Maintaining-a-Source-Checkout.rst:201
