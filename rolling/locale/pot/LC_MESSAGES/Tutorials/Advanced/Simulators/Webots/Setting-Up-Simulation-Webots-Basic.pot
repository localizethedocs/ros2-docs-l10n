"  SHARED\n"
"  src/MyRobotDriver.cpp\n"
")\n"
"target_include_directories(\n"
"  ${PROJECT_NAME}\n"
"  PRIVATE\n"
"  include\n"
")\n"
"ament_target_dependencies(\n"
"  ${PROJECT_NAME}\n"
"  pluginlib\n"
"  rclcpp\n"
"  webots_ros2_driver\n"
")\n"
"install(TARGETS\n"
"  ${PROJECT_NAME}\n"
